{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "coreui/utils": ["../../core-ui/src/modules/common/utils"],
      "coreui/apolloClient": ["../../core-ui/src/apolloClient.ts"],
      "coreui/pluginUtils": ["../../core-ui/src/pluginUtils"],
      "coreui/appContext": ["../../core-ui/src/appContext"],
      "coreui/withPermission": ["../../core-ui/src/modules/common/components/WithPermission"],
      "coreui/*": ["../../core-ui/src/*"]
    },
    "outDir": "build/dist",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom",
      "es2018",
      "esnext.asynciterable"
    ],
    "sourceMap": true,
    "jsx": "react",
    "rootDir": "src",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "webpack"
  ]
}